---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in site.posts %}<url>
    <loc>{{ post.url | prepend: site.baseurl | prepend: site.url }}</loc>
    {% capture year %}{{post.date | date: "%Y" }}{% endcapture %}{% if year > "2010" %}<lastmod>{{ post.date | date_to_xmlschema }}</lastmod>{% else %}<lastmod>{{ site.default_sitemap_lastmod | date_to_xmlschema }}</lastmod>{% endif %}
  </url>
  {% endfor %}{% for page in site.html_pages %}{% if page.include_in_search %}<url>
    <loc>{{ page.url | prepend: site.baseurl | prepend: site.url }}</loc>
    <lastmod>{{ site.default_sitemap_lastmod | date_to_xmlschema }}</lastmod>
  </url>
  {% endif %}{% endfor %}</urlset>
